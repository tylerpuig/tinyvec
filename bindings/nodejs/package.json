{
  "name": "tinyvecdb",
  "version": "0.2.3",
  "description": "TinyVecDB is a high performance, lightweight, embedded vector database for similarity search.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src",
    "dist",
    "binding.gyp",
    "prebuilds",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "build:dev": "tsx scripts/transferCore.ts && prebuildify --napi --strip",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "prebuildify-all": "prebuildify --napi --strip --all"
  },
  "keywords": [
    "tinyvec",
    "vector",
    "database",
    "similarity",
    "search",
    "vector database",
    "vector search",
    "similarity search",
    "embedded vector database"
  ],
  "author": "tylerpuig",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "prebuildify": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp": "^11.1.0",
    "node-gyp-build": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerpuig/tinyvec.git"
  },
  "gypfile": true
}
